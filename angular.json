{
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "george-knap": {
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "wwwroot",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              {
                "glob": "assets",
                "input": "/src",
                "output": "/"
              },
              {
                "glob": "i18n",
                "input": "/src",
                "output": "/"
              },
              {
                "glob": "manifest.json",
                "input": "/src",
                "output": "/"
              }
            ],
            "styles": [
              {
                "input": "src/styles/app.scss"
              },
              {
                "input": "node_modules/material-design-icons/iconfont/material-icons.css"
              },
              {
                "input": "node_modules/ag-grid/src/styles/ag-grid.scss"
              },
              {
                "input": "node_modules/ag-grid/src/styles/ag-theme-material.scss"
              }
            ],
            "scripts": [
              {
                "input": "node_modules/lodash/lodash.js"
              },
              {
                "input": "node_modules/hammerjs/hammer.js"
              }
            ]
          },
          "configurations": {
            "dev": {
              "fileReplacements": [
                {
                  "from": "src/environments/environment.ts",
                  "to": "wwwroot/environments/environment.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "from": "src/environments/environment.ts",
                  "to": "wwwroot/undefined"
                }
              ]
            },
            "hmr": {
              "fileReplacements": [
                {
                  "from": "src/environments/environment.ts",
                  "to": "wwwroot/environments/environment.hmr.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "george-knap:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "george-knap:build:dev"
            },
            "production": {
              "browserTarget": "george-knap:build:production"
            },
            "hmr": {
              "browserTarget": "george-knap:build:hmr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "george-knap:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "karmaConfig": "karma.conf.js",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              {
                "input": "node_modules/jquery/dist/jquery.slim.js"
              },
              {
                "input": "node_modules/lodash/lodash.js"
              },
              {
                "input": "node_modules/hammerjs/hammer.js"
              }
            ],
            "styles": [
              {
                "input": "src/styles/app.scss"
              },
              {
                "input": "node_modules/material-design-icons/iconfont/material-icons.css"
              },
              {
                "input": "node_modules/ag-grid/src/styles/ag-grid.scss"
              },
              {
                "input": "node_modules/ag-grid/src/styles/ag-theme-material.scss"
              }
            ],
            "assets": [
              {
                "glob": "assets",
                "input": "/src",
                "output": "/"
              },
              {
                "glob": "i18n",
                "input": "/src",
                "output": "/"
              },
              {
                "glob": "manifest.json",
                "input": "/src",
                "output": "/"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**/*"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "gk",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "gk"
    }
  }
}